{
  "schemaVersion": 1,
  "id": "itemcleaner",
  "version": "${version}",
  "name": "ItemCleaner",
  "description": "Custom drop cleaning tool, supporting scheduled cleaning and range configuration",
  "authors": ["StellarVan"],
  "contact": {
    "homepage": "https://github.com/mcheheyixiao"},
  "license": "MIT",
  "icon": "assets/itemcleaner/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.stellarvan.itemcleaner.client.ItemcleanerDataGenerator"
    ],
    "client": [
      "org.stellarvan.itemcleaner.client.ItemcleanerClient"
    ],
    "main": [
      "org.stellarvan.itemcleaner.ItemCleaner"
    ]
  },
  "mixins": [
    "itemcleaner.mixins.json",
    {
      "config": "itemcleaner.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
